var x : int;
var y : float;

function fa(x : string; y[3] : char; z : bool) : int
{
	x = "abc";
	y[0] = 1.0;
	fb(z);
	
	return 0;
}

function fb(x : bool) : void
{
    y = x;
	x = !x;
	
	return;
}

main()
{
    var x : string;
	var z[3] : float;
	z[0] = x;
	z[1] = y;
	z[2] = z[0];
    x = fa("xyz", z, false);
}
